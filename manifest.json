{
  "manifest_version": 3,
  "name": "Typing Malayalam Judge",
  "version": "1.0",
  "description": "Plays Malayalam insults or praise based on typing speed",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["audio/*.mp3"],
      "matches": ["<all_urls>"]
    }
  ]
}
